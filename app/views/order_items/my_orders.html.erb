<div class="row">
    <div class="col-xs-8 col-sm-10 col-md-10 text-center">
        <h1>Ordenes</h1>
    </div>
</div>

<table>
  <thead>
    <tr>
       <th>id</th>
       <th>created_at</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order|%>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>